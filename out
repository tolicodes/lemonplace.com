[33;1mmysql_lemonplace |[0m comments                          FALSE
[33;1mmysql_lemonplace |[0m compress                          FALSE
[33;1mmysql_lemonplace |[0m database                          (No default value)
[33;1mmysql_lemonplace |[0m default-character-set             auto
[33;1mmysql_lemonplace |[0m delimiter                         ;
[33;1mmysql_lemonplace |[0m enable-cleartext-plugin           FALSE
[33;1mmysql_lemonplace |[0m vertical                          FALSE
[33;1mmysql_lemonplace |[0m force                             FALSE
[33;1mmysql_lemonplace |[0m histignore                        (No default value)
[33;1mmysql_lemonplace |[0m named-commands                    FALSE
