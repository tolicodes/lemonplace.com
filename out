[33;1mmysql_lemonplace |[0m init-command                      (No default value)
[33;1mmysql_lemonplace |[0m local-infile                      FALSE
[33;1mmysql_lemonplace |[0m no-beep                           FALSE
[33;1mmysql_lemonplace |[0m host                              mysql_lemonplace
[33;1mmysql_lemonplace |[0m html                              FALSE
[33;1mmysql_lemonplace |[0m xml                               FALSE
[33;1mmysql_lemonplace |[0m line-numbers                      TRUE
[33;1mmysql_lemonplace |[0m unbuffered                        FALSE
[33;1mmysql_lemonplace |[0m column-names                      TRUE
[33;1mmysql_lemonplace |[0m sigint-ignore                     FALSE
[33;1mmysql_lemonplace |[0m port                              0
[33;1mmysql_lemonplace |[0m prompt                            mysql> 
[33;1mmysql_lemonplace |[0m quick                             FALSE
[33;1mmysql_lemonplace |[0m raw                               FALSE
[33;1mmysql_lemonplace |[0m reconnect                         FALSE
[33;1mmysql_lemonplace |[0m socket                            (No default value)
[33;1mmysql_lemonplace |[0m ssl                               TRUE
[33;1mmysql_lemonplace |[0m ssl-verify-server-cert            FALSE
[33;1mmysql_lemonplace |[0m ssl-ca                            (No default value)
[33;1mmysql_lemonplace |[0m ssl-capath                        (No default value)
[33;1mmysql_lemonplace |[0m ssl-cert                          (No default value)
[33;1mmysql_lemonplace |[0m ssl-cipher                        (No default value)
[33;1mmysql_lemonplace |[0m ssl-key                           (No default value)
[33;1mmysql_lemonplace |[0m ssl-crl                           (No default value)
[33;1mmysql_lemonplace |[0m ssl-crlpath                       (No default value)
[33;1mmysql_lemonplace |[0m tls-version                       (No default value)
[33;1mmysql_lemonplace |[0m table                             FALSE
[33;1mmysql_lemonplace |[0m user                              root
[33;1mmysql_lemonplace |[0m safe-updates                      FALSE
[33;1mmysql_lemonplace |[0m i-am-a-dummy                      FALSE
[33;1mmysql_lemonplace |[0m connect-timeout                   0
[33;1mmysql_lemonplace |[0m max-allowed-packet                16777216
[33;1mmysql_lemonplace |[0m net-buffer-length                 16384
[33;1mmysql_lemonplace |[0m select-limit                      1000
[33;1mmysql_lemonplace |[0m max-join-size                     1000000
[33;1mmysql_lemonplace |[0m secure-auth                       TRUE
[33;1mmysql_lemonplace |[0m show-warnings                     FALSE
[33;1mmysql_lemonplace |[0m plugin-dir                        (No default value)
